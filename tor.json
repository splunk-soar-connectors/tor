    "main_module": "tor_connector.pyc",
    "min_phantom_version": "4.0.1068",
    "app_wizard_version": "1.0.0",
    "configuration": {},
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Validates the connection to the Tor Project website",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "lookup ip",
            "identifier": "lookup_ip",
            "description": "Check if IP is a Tor exit node",
            "verbose": "Download a list of current exit nodes to determine if an IP is an exit node. During each action run, if the current list is found to be downloaded over 30 minutes ago, it will download an updated version.",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "ip": {
                    "description": "IP or list of IPs",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "ip"
                    ],
                    "allow_list": true,
                    "order": 0
                }
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success"
                    ]
                },
                {
                    "data_path": "action_result.parameter.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "example_values": [
                        "195.154.251.25"
                    ]
                },
                {
                    "data_path": "action_result.data.*.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "example_values": [
                        "84.105.18.164",
                        "195.154.251.25"
                    ],
                    "column_name": "IP",
                    "column_order": 0
                },
                {
                    "data_path": "action_result.data.*.is_exit_node",
                    "data_type": "boolean",
                    "example_values": [
                        true,
                        false
                    ],
                    "column_name": "Is Exit Node",
                    "column_order": 1
                },
                {
                    "data_path": "action_result.summary.num_exit_nodes",
                    "data_type": "numeric",
                    "example_values": [
                        2,
                        1
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string",
                    "example_values": [
                        "Successfully investigated IPs"
                    ]
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                }
            ],
            "render": {
                "type": "table"
            },
            "versions": "EQ(*)"
        }
    ]
}