{
    "appid": "2595f157-1e11-4287-b13a-655e553e9a8d",
    "name": "Tor",
    "description": "This app implements investigative actions to query info about the Tor network",
    "type": "information",
    "product_vendor": "Tor",
    "logo": "logo_tor.svg",
    "logo_dark": "logo_tor_dark.svg",
    "product_name": "Tor",
    "product_version_regex": ".*",
    "publisher": "Splunk",
    "license": "Copyright (c) 2017-2022 Splunk Inc.",
    "app_version": "2.0.3",
    "utctime_updated": "2022-01-07T21:45:32.000000Z",
    "package_name": "phantom_tor",
    "main_module": "tor_connector.py",
    "min_phantom_version": "5.1.0",
    "fips_compliant": true,
    "python_version": "3",
    "latest_tested_versions": [
        "On local cloud, tested on 1st June 2021"
    ],
    "app_wizard_version": "1.0.0",
    "configuration": {},
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Validates the connection to the Tor Project website",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "lookup ip",
            "identifier": "lookup_ip",
            "description": "Check if IP is a Tor exit node",
            "verbose": "Download a list of current exit nodes to determine if an IP is an exit node. During each action run, if the current list is found to be downloaded over 30 minutes ago, it will download an updated version.",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "ip": {
                    "description": "IP or list of IPs",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "ip"
                    ],
                    "allow_list": true,
                    "order": 0
                }
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.parameter.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "example_values": [
                        "195.154.251.25"
                    ]
                },
                {
                    "data_path": "action_result.data.*.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "example_values": [
                        "84.105.18.164",
                        "195.154.251.25"
                    ],
                    "column_name": "IP",
                    "column_order": 0
                },
                {
                    "data_path": "action_result.data.*.is_exit_node",
                    "data_type": "boolean",
                    "example_values": [
                        true,
                        false
                    ],
                    "column_name": "Is Exit Node",
                    "column_order": 1
                },
                {
                    "data_path": "action_result.summary.num_exit_nodes",
                    "data_type": "numeric",
                    "example_values": [
                        2,
                        1
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string",
                    "example_values": [
                        "Successfully investigated IPs"
                    ]
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                }
            ],
            "render": {
                "type": "table"
            },
            "versions": "EQ(*)"
        }
    ],
    "pip_dependencies": {
        "wheel": [
            {
                "module": "certifi",
